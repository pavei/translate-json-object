{
  "name": "asksuite-translate-json-object",
  "version": "0.0.26",
  "description": "Translate a JSON object to any language via google or yandex translate API",
  "main": "lib/translate-json-object.js",
  "scripts": {
    "lint": "xo",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md lib/translate-json-object.js > docs/api.md"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "google-translate": "2.0.5",
    "lodash": "4.17.3",
    "md5": "^2.2.1",
    "mstranslator": "^3.0.0",
    "promise": "7.1.1",
    "request-json": "^0.6.2",
    "sync-promise": "^1.1.0",
    "yandex-translate": "2.1.1"
  },
  "devDependencies": {
    "commitizen": "2.9.3",
    "cz-conventional-changelog": "1.2.0",
    "husky": "0.12.0",
    "jest": "18.0.0",
    "jsdoc-to-markdown": "2.0.1",
    "semantic-release": "6.3.2",
    "validate-commit-msg": "2.8.2",
    "xo": "0.17.1"
  },
  "xo": {
    "global": [
      "jest",
      "expect",
      "test",
      "describe",
      "it"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "translate",
    "object",
    "JSON",
    "google",
    "yandex",
    "translate"
  ],
  "author": "Khaled Mohamed",
  "license": "MIT",
  "homepage": "https://github.com/KhaledMohamedP/translate-json-object#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/KhaledMohamedP/translate-json-object.git"
  },
  "bugs": {
    "url": "https://github.com/KhaledMohamedP/translate-json-object/issues"
  }
}
